</form>

@section Scripts
{
  <script>
    $(document).ready(function () {
      $("#enterName").val(@Model.credit.fullName);
      $("#month").val(@Model.credit.expiredDate);
      $("#year").val(@Model.credit.expiredYear);
      $("#enterCredit").val(@Model.credit.creditNum);
      $("#enterCode").val(@Model.credit.securityCode);
      $("#childCost").text(@Model.package.flight1.child + @Model.package.flight2.child);
      $("#adultCost").text(@Model.package.flight1.adult + @Model.package.flight2.adult);
      $("#elderCost").text(@Model.package.flight1.elder + @Model.package.flight2.elder);
    });
  </script>
  <script>
    $("#submit").click(function(event) {
      if ($("#credSave").is(":checked"))
      {
        $("#saveCredit").val(1);
      }
      else
      {
        $("#saveCredit").val(0);
      }
    });
    $("#addChild").click(function(event) {
      var child = parseInt($("#childCount").text().trim(), 10);
      var adult = parseInt($("#adultCount").text().trim(), 10);
      var elder = parseInt($("#elderCount").text().trim(), 10);
      var childCost = @Model.package.flight1.child;
      child = child + 1;
      var prePrice = (child * @Model.package.flight1.child) + (adult * @Model.package.flight1.adult) + (elder * @Model.package.flight1.elder);
      var taxes = (prePrice * 10) / 100;
      var postPrice = prePrice + taxes + 35;
      var ticket = child + adult + elder;
      if (child <= 9)
      {
        $("#childCount").text(child);
        $("#childCostSum").text((child * childCost) + "$");
        $("#child"+child).show();
        $("#credit").show();
        $("#prePrice").text(prePrice);
        $("#taxes").text(taxes);
        $("#postPrice").text(postPrice);
        $("#ticketCount").val(ticket);
      }
    });
    $("#removeChild").click(function(event) {
      var child = parseInt($("#childCount").text().trim(), 10);
      var adult = parseInt($("#adultCount").text().trim(), 10);
      var elder = parseInt($("#elderCount").text().trim(), 10);
      var childCost = @Model.package.flight1.child;
      child = child - 1;
      var prePrice = (child * @Model.package.flight1.child) + (adult * @Model.package.flight1.adult) + (elder * @Model.package.flight1.elder);
      var taxes = (prePrice * 10) / 100;
      var postPrice = prePrice + taxes + 35;
      var ticket = child + adult + elder;
      if (child >= 0)
      {
        $("#childCount").text(child);
        $("#childCostSum").text((childCost * child) + "$");
        $("#child"+(child + 1)).hide();
        $("#prePrice").text(prePrice);
        $("#taxes").text(taxes);
        $("#postPrice").text(postPrice);
        $("#ticketCount").val(ticket);
        if (child == 0 && adult == 0 && elder == 0)
        {
          $("#credit").hide();
        }
      }
    });
    $("#addAdult").click(function(event) {
      var child = parseInt($("#childCount").text().trim(), 10);
      var adult = parseInt($("#adultCount").text().trim(), 10);
      var elder = parseInt($("#elderCount").text().trim(), 10);
      var adultCost = @Model.package.flight1.adult;
      adult = adult + 1;
      var prePrice = (child * @Model.package.flight1.child) + (adult * @Model.package.flight1.adult) + (elder * @Model.package.flight1.elder);
      var taxes = (prePrice * 10) / 100;
      var postPrice = prePrice + taxes + 35;
      var ticket = child + adult + elder;
      if (adult <= 9)
      {
        $("#adultCount").text(adult);
        $("#adultCostSum").text((adult * adultCost) + "$");
        $("#adult"+adult).show();
        $("#credit").show();
        $("#prePrice").text(prePrice);
        $("#taxes").text(taxes);
        $("#postPrice").text(postPrice);
        $("#ticketCount").val(ticket);
      }
    });
    $("#removeAdult").click(function(event) {
      var child = parseInt($("#childCount").text().trim(), 10);
      var adult = parseInt($("#adultCount").text().trim(), 10);
      var elder = parseInt($("#elderCount").text().trim(), 10);
      var adultCost = @Model.package.flight1.adult;
      adult = adult - 1;
      var prePrice = (child * @Model.package.flight1.child) + (adult * @Model.package.flight1.adult) + (elder * @Model.package.flight1.elder);
      var taxes = (prePrice * 10) / 100;
      var postPrice = prePrice + taxes + 35;
      var ticket = child + adult + elder;
      if (adult >= 0)
      {
        $("#adultCount").text(adult);
        $("#adultCostSum").text((adult * adultCost) + "$");
        $("#adult"+(adult + 1)).hide();
        $("#prePrice").text(prePrice);
        $("#taxes").text(taxes);
        $("#postPrice").text(postPrice);
        $("#ticketCount").val(ticket);
        if (child == 0 && adult == 0 && elder == 0)
        {
          $("#credit").hide();
        }
      }
    });
    $("#addElder").click(function(event) {
      var child = parseInt($("#childCount").text().trim(), 10);
      var adult = parseInt($("#adultCount").text().trim(), 10);
      var elder = parseInt($("#elderCount").text().trim(), 10);
      var elderCost = @Model.package.flight1.elder;
      elder = elder + 1;
      var prePrice = (child * @Model.package.flight1.child) + (adult * @Model.package.flight1.adult) + (elder * @Model.package.flight1.elder);
      var taxes = (prePrice * 10) / 100;
      var postPrice = prePrice + taxes + 35;
      var ticket = child + adult + elder;
      if (elder <= 9)
      {
        $("#elderCount").text(elder);
        $("#elderCostSum").text((elder * elderCost) + "$");
        $("#elder"+elder).show();
        $("#credit").show();
        $("#prePrice").text(prePrice);
        $("#taxes").text(taxes);
        $("#postPrice").text(postPrice);
        $("#ticketCount").val(ticket);
      }
    });
    $("#removeElder").click(function(event) {
      var child = parseInt($("#childCount").text().trim(), 10);
      var adult = parseInt($("#adultCount").text().trim(), 10);
      var elder = parseInt($("#elderCount").text().trim(), 10);
      var elderCost = @Model.package.flight1.elder;
      elder = elder - 1;
      var prePrice = (child * @Model.package.flight1.child) + (adult * @Model.package.flight1.adult) + (elder * @Model.package.flight1.elder);
      var taxes = (prePrice * 10) / 100;
      var postPrice = prePrice + taxes + 35;
      var ticket = child + adult + elder;
      if (elder >= 0)
      {
        $("#elderCount").text(elder);
        $("#elderCostSum").text((elder * elderCost) + "$");
        $("#elder"+(elder + 1)).hide();
        $("#prePrice").text(prePrice);
        $("#taxes").text(taxes);
        $("#postPrice").text(postPrice);
        $("#ticketCount").val(ticket);
        if (child == 0 && adult == 0 && elder == 0)
        {
          $("#credit").hide();
        }
      }
    });
  </script>
}